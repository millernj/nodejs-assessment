{
  "name": "nodejs-assessment",
  "version": "0.0.1",
  "description": "This project is the assessment for Nodejs developers",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/src/index",
    "clean": "rimraf dist",
    "build": "tsc",
    "dev": "npm-run-all --parallel watch:build watch:server",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon dist/src/index --ignore data/*",
    "test": "jest",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dynogg/nodejs-assessment.git"
  },
  "keywords": [
    "node",
    "javascript",
    "assessment"
  ],
  "author": "Anderson Menezes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dynogg/nodejs-assessment/issues"
  },
  "homepage": "https://github.com/dynogg/nodejs-assessment#readme",
  "_moduleAliases": {
    "@": "dist/src",
    "data": "data"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "rimraf": "^5.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5"
  }
}
